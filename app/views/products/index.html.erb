<header>
  <ul>
    <li><%= link_to 'Sign in', signup_path %></li>
    <li><%= link_to 'Sign up', signup_path %></li>
  </ul>
</header>

<h1>Products</h1>



<table>

<% @products.each do |product| %>
  <tr>
    <td><h4><%= product.name %></h4></td>
    <td>Price: <%= number_to_currency product.price %></td>
    <td>Manufacturer: <%= product.manufacturer %></td>
  </tr>
<% end %>
</table>
